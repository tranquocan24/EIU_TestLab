{
  "name": "eiu-testlab",
  "version": "1.0.0",
  "description": "EIU TestLab - Hệ thống thi trực tuyến của Đại học Quốc tế Miền Đông",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build:backend && npm run build:frontend",
    "test": "node test-complete-flow.js",
    "test:basic": "node test-basic-flow.js",
    "test:ci": "node test-basic-flow.js",
    "test:full": "node test-complete-flow.js",
    "test:report": "node test-complete-flow.js && cat test-report.json"
  },
  "keywords": [
    "nodejs",
    "exam",
    "online",
    "education",
    "quiz",
    "test",
    "EIU",
    "testlab",
    "university"
  ],
  "author": "EIU TestLab Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "homepage": "http://localhost:3000",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "puppeteer": "^24.12.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}